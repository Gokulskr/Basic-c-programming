#include<stdio.h>
int main()
{
    int arr[5],i;
    for(i=0;i<5;i++)
    {
        printf("Enter the value for arr[%d]\n",i);
        scanf("%d",&arr[i]);
    }
    printf("the array elements are :\n");
    for(i=0;i<5;i++)
    {
        printf("%d\t",arr[i]);
        
    }
    return 0;
}

output
Enter the value for arr[0]
15
Enter the value for arr[1]
15
Enter the value for arr[2]
15
Enter the value for arr[3]
15
Enter the value for arr[4]
15
the array elements are :
15      15      15      15      15
