#include<stdio.h>
#include<string.h>
int main()
{
   char s1[20]="beginers book";
   char s2[20]="beginers book";
   if(strcmp (s1,s2)==0)
   {
       printf("the string 1 and 2 are same");
   }
   else
   {
       printf("the string 1 and two are different");
   }
   return 0;
}

output
the string 1 and 2 are same
