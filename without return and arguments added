#include<stdio.h>
void average(int,int,int,int,int);
void main()
{
	int a,b,c,d,e;
	printf("Going to calculate the average of the numbers");
	printf("\nEnter five numbers:");
	scanf("%d%d%d%d%d",&a,&b,&c,&d,&e);
	average(a,b,c,d,e);
	
}
void average(int a, int b,int c,int d,int e)
{
	float avg;
	avg=(a+b+c+e)/5;
	printf("The average of five number:%f",avg);
}

output
Going to calculate the average of the numbers
Enter five numbers:1 2 3 4 5
The average of five number:2.000000
